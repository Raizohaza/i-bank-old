    "start:services": "concurrently \"npm run start:token\" \"npm run start:customer\" \"npm run start:account\" \"npm run start:transaction\" \"npm run start:employee\" \"npm run start:linking-bank\" \"npm run start:receivers\" \"npm run start:debt\"",
